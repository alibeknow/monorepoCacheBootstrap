{
  "name": "unleashed-capital",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "src/gateway/main.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gateway": "npx nest start gateway --watch",
    "transaction": "npx nest start transaction --watch",
    "account": "ts-node -r tsconfig-paths/register src/account/main.ts account",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx jest -c jest.config.js",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:docker": "docker-compose up -d",
    "stop:docker": "docker-compose down -v",
    "start:rabbit": "docker-compose up -d rabbitmq",
    "start:pg": "docker-compose up -d postgres",
    "typeorm": "ts-node node_modules/typeorm/cli.js -f ormconfig",
    "migration:generate": "npm run typeorm migration:generate -- --name init",
    "migration:create": "npm run typeorm migration:create -d migrations -n init",
    "migration:revert": "npm run typeorm migration:revert",
    "seed:config": "ts-node node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.3",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/graphql": "^10.1.3",
    "@nestjs/microservices": "^9.1.4",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@types/amqplib": "^0.8.2",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/compression": "^1.7.2",
    "@types/faker": "^6.6.9",
    "@types/redis": "^2.8.32",
    "@types/webpack-env": "^1.18.0",
    "amqp-connection-manager": "^4.1.7",
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.10.3",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "faker": "^6.6.6",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "node-cache-manager": "^1.2.0",
    "pg": "^8.8.0",
    "pino": "^8.6.1",
    "pm": "^2.2.6",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "typeorm": "^0.3.10",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/express": "^4.17.14",
    "@types/jest": "29.1.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.5",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
